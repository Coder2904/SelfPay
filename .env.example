git# SelfPay Environment Variables
# Copy this file to .env and fill in your actual values

# Plaid Configuration
EXPO_PUBLIC_PLAID_CLIENT_ID=your_plaid_client_id_here
EXPO_PUBLIC_PLAID_SECRET=your_plaid_secret_here

# RevenueCat Configuration
EXPO_PUBLIC_REVENUECAT_API_KEY=your_revenuecat_api_key_here

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Development Configuration
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_DEBUG_MODE=true

# Optional: Analytics and Monitoring (for future use)
EXPO_PUBLIC_ANALYTICS_API_KEY=your_analytics_api_key_here
EXPO_PUBLIC_CRASH_REPORTING_KEY=your_crash_reporting_key_here